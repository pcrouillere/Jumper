{
  "manifest_version": 2,
  "name": "Jumper",
  "description": "How to deal with Bookmarks",
  "version": "1.1",
  "icons":
		{ 	"16": "icones/icone16.png",
			"48": "icones/icone48.png",
			"128": "icones/icone128.png"
		},
  "permissions": [ "cookies", "tabs", "http://*/*", "https://*/*", "activeTab" ],
  "browser_action": {
    "default_icon": "icones/icone19.png",
    "default_popup": "extension.html"
  }
}
